// C++ program to sort a string of characters 
#include<bits/stdc++.h> 
#include<stdio.h>
using namespace std; 

// function to print string in sorted order 
void sortString(string &str) 
{ 
sort(str.begin(), str.end()); 
reverse(str.begin(), str.end()); 
  
cout << str; 
} 

// Driver program to test above function 
int main() 
{ 
	int t;
	cin>>t;
	while(t--)
	{ string s;
	  cin>>s;
	sortString(s); 
	cout<<endl;
	}
	return 0; 
} 
